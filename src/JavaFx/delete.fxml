         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="textFieldXoa" layoutX="148.0" layoutY="113.0" onKeyPressed="#keyPress" prefHeight="37.0" prefWidth="215.0" style="-fx-border-color: blue;">
